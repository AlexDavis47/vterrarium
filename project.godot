; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VTerrarium"
run/main_scene="res://src/game_world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

VTGlobal="*res://addons/vterrariumframework/src/globals/vtglobal.gd"
VTConfig="*res://addons/vterrariumframework/src/globals/vtconfig.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/resizable=false
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="VTerrarium"

[editor_plugins]

enabled=PackedStringArray("res://addons/vterrariumframework/plugin.cfg")

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[rendering]

textures/canvas_textures/default_texture_filter=3
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
