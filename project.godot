; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VTerrarium"
run/main_scene="res://src/game_world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

VTGlobal="*res://addons/vterrariumframework/src/globals/vtglobal.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/mode=3
window/size/initial_position_type=2
window/size/resizable=false
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="VTerrarium"

[editor_plugins]

enabled=PackedStringArray("res://addons/vterrariumframework/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
