; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VTerrarium"
run/main_scene="res://game_world.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/mode=3
window/size/borderless=true
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="VTerrarium"

[editor_plugins]

enabled=PackedStringArray("res://addons/vterrariumframework/plugin.cfg")

[layer_names]

3d_render/layer_1="PrimaryScreen"
3d_render/layer_2="SecondaryScreen"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
